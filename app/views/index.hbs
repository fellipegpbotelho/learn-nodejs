<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<p>Hello, {{name}}</p>

<table>
    <thead>
    <tr>
        <th>id</th>
        <th>name</th>
        <th>date</th>
    </tr>
    </thead>
    <tbody>
    {{#each companies}}
    <tr>
        <td>{{id}}</td>
        <td>{{name}}</td>
        <td>{{#date}}{{/date}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>
